def shipping_label(*args, **kwargs):
    for arg in args:
        print(arg,end=" ")
    print()
    if "apt" in kwargs:
        print(f"{kwargs.get('street')} {kwargs.get('apt')}")
    else:
        print(f"{kwargs.get('steet')}")
    print(f"{kwargs.get('city')} {kwargs.get('state')}, {kwargs.get('zip')}")

    


shipping_label("Dr.", "Spongebob", "squarepants","III",
street = "123 Fake St.",
apt = "100",
city = "detroit",
state = "MI",
zip = "54928")